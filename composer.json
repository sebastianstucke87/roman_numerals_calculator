{
  "name": "sebastianstucke87/roman_numerals_calculator",
  "description": "A basic calculator for roman numerals from 'I (1)' to 'MMMCMXCIX (3999)'",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Stucke",
      "email": "sebastianstucke87@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "beberlei/assert": "^3.2",
    "thecodingmachine/safe": "^1.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "RomanNumeralsCalculator\\": "src/RomanNumeralsCalculator"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RomanNumeralsCalculator\\": "tests/RomanNumeralsCalculator"
    }
  },
  "config": {
    "apcu-autoloader": true,
    "discard-changes": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "7.4"
    }
  }
}
